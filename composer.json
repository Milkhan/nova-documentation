{
    "name": "milkhan/nova-help",
    "description": "A Laravel Nova tool that allows you to add markdown-based help to your administrator's dashboard based on Doug Niccum's Documentation tool.",
    "homepage": "https://github.com/milkhan/nova-help",
    "keywords": [
        "laravel",
        "nova",
        "markdown",
        "help"
    ],
    "authors": [
        {
            "name": "Mustafa ilkhan",
            "email": "milkhan@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "cebe/markdown": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Milkhan\\NovaHelp\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Milkhan\\NovaHelp\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
